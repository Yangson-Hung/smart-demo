.page {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.header {
  font-size: 60rpx;
  font-weight: bold;
  margin-top: 50rpx;
}
.body {
  background-color: white;
  border-radius: 20rpx;
  box-shadow: 0rpx 3rpx 6rpx rgba(26, 26, 26, 0.2);
}

.box {
  font-size: 35rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding-top: 40rpx;
}

.title {
  padding-left: 40rpx;
}
input {
  padding-left: 20rpx;
}
